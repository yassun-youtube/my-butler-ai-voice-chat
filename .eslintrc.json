{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-unused-vars": "off",
    "semi": [
      "error",
      "never"
    ],
    "prettier/prettier": [
      "error",
      {
        "semi": false
      }
    ]
  },
  "plugins": [
    "prettier"
  ]
}
